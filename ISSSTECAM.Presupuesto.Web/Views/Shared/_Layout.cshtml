                                <li class="sidebar-subnav-header">Catálogos</li>
                                <li>
                                    <a href="@Url.Action("Index", "CentrosCostos")" class="ajax">
                                        <span>Centros de costos</span>
                                    </a>
                                </li>

                                <li class="sidebar-subnav-header">Conceptos de nómina</li>
                                <li>
                                    <a href="@Url.Action("Conceptos", "Nomina")" class="ajax">                                        
                                        <span>Conceptos de nómina</span>
                                    </a>
                                </li>
                            </ul>

                            <a href="#Presupuesto" title="Presupuesto" data-toggle="collapse">
                                <em class="fa fa-calendar"></em>
                                <span data-localize="sidebar.nav.Presupuesto">Presupuesto</span>
                            </a>
                            <ul id="Presupuesto" class="nav sidebar-subnav collapse in" aria-expanded="true">
                                <li class="sidebar-subnav-header">Configuración</li>
                                <li>
                                    <a href="@Url.Action("Index", "Configuraciones")" class="ajax">
                                        <span>Configuración</span>
                                    </a>
                                </li>

                                <li class="sidebar-subnav-header">Calendario</li>
                                <li>
                                    <a href="@Url.Action("Index", "Calendario")" class="ajax">
                                        <span>Calendario</span>
                                    </a>
                                </li>

                                <li class="sidebar-subnav-header">Captura</li>
                                <li>
                                    <a href="@Url.Action("Captura", "ClavesPresupuestales")" class="ajax">
                                        <span>Captura</span>
                                    </a>
                                </li>
                            </ul>


                            <a href="#Solicitudes" title="Solicitudes" data-toggle="collapse">
                                <em class="fa fa-paperclip"></em>
                                <span data-localize="sidebar.nav.Solicitudes">Solicitudes</span>
                            </a>
                            <ul id="Solicitudes" class="nav sidebar-subnav collapse in" aria-expanded="true">
                                <li class="sidebar-subnav-header">Listado</li>
                                <li>
                                    <a href="@Url.Action("Index", "Solicitudes")" class="ajax">                                        
                                        <span>Listado</span>
                                    </a>
                                </li>

                                <li class="sidebar-subnav-header">Operaciones</li>
                                <li>
                                    <a href="@Url.Action("Operaciones", "Solicitudes")" class="ajax">                                        
                                        <span>Operaciones</span>
                                    </a>
                                </li>

                                @*<li class="sidebar-subnav-header">Calendario</li>
                                <li>
                                    <a href="@Url.Action("Index", "Calendario")" class="ajax">
                                        <span>Calendario</span>
                                    </a>
                                </li>*@
                            </ul>




                            <a href="#Nomina" title="Nómina" data-toggle="collapse">
                                <em class="fa fa-paperclip"></em>
                                <span data-localize="sidebar.nav.Nomina">Nómina</span>
                            </a>
                            <ul id="Nomina" class="nav sidebar-subnav collapse in" aria-expanded="true">

                                @*<li class="sidebar-subnav-header">Catálogo de conceptos</li>
                                <li>
                                    <a href="@Url.Action("Index", "Nomina")" class="ajax">                                        
                                        <span>Catálogo de conceptos</span>
                                    </a>
                                </li>*@

                                <li class="sidebar-subnav-header">Cargar nómina</li>
                                <li>
                                    <a href="@Url.Action("Cargar", "Nomina")" class="ajax">                                        
                                        <span>Cargar nómina</span>
                                    </a>
                                </li>
                                
                            </ul>
                        </li>
                    </ul>
                    <!-- END Menú lateral nav-->
                </nav>
            </div>
            <!-- FIN Menú lateral izquierdo-->
        </aside>
        <section class="section-container">
            <div class="content-wrapper" id="cuerpo">
                <div class="content-heading">
                    <div class="pull-right ">
                        <div class="nombresistema">
                            <div class="logo" style="font-size: 1.4em;">
                                <em class="fa fa-cube"></em>
                            </div>
                            <div class="nombrelogo">
                                <em class="fa fa-cube" style="font-size: 1.4em;"></em>&nbsp Sistema
                            </div>
                        </div>
                    </div>

                    @*<asp:contentplaceholder id="Titulo" runat="server"></asp:contentplaceholder>*@
                    @RenderSection("titulo", required: false)
                </div>
                <div class="row acciones">
                    <div class="pull-right">
                        @RenderSection("acciones", required: false)
                        @*<asp:ContentPlaceHolder ID="Acciones" runat="server"></asp:ContentPlaceHolder>*@
                    </div>
                </div>
                <div class="row">
                    @*<asp:ContentPlaceHolder ID="Contenido" runat="server">
                    </asp:ContentPlaceHolder>*@
                    @RenderBody()
                </div>
            </div>
        </section>
        <footer class="footer-container">
            <div class="pull-left" style="width: 60%">
                <div>
                    <img src="@Url.Content("~/Content/images/escudoCampeche.png")" style="float: left; height: 65px;">
                    <div class="nombrelogo">Instituto de Seguridad y Servicios Sociales de los Trabajadores del Estado de Campeche</div>
                    <div class="logo">ISSSTECAM</div>
                    <div>
                        Calle 10 s/n entre 63 y 65, Col. Centro San Francisco de Campeche, Campeche 
                    </div>
                </div>
                <div style="float: left">

                    <a href="https://issstecam.gob.mx/" target="_blank" title="Sitio" data-toggle="tooltip" data-placement="bottom">
                        <em class="fa fa-globe fa-1_6x text-primary"></em>
                    </a>

                    &nbsp
                    <a title="Facebook" class="links" target="_blank" onclick="" href="https://www.facebook.com/ISSSTECAM/">
                        <em class="fa fa-facebook-square fa-1_6x text-primary"></em>
                    </a>
                    &nbsp
                    <a title="Twitter" class="links" target="_blank" onclick="" href="https://twitter.com/ISSSTECAM">
                        <em class="fa fa-twitter fa-1_6x text-info"></em>
                    </a>
                </div>
            </div>
            <div class="pull-right">
                <div class="nombresistema">
                    <div class="nombrelogo">
                        <img src="@Url.Content("~/Content/images/issstecam_logo.png")" style="height: 55px" />
                    </div>
                    <div class="logo">
                    </div>
                </div>
            </div>
        </footer>
