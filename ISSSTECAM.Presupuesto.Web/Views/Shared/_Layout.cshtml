<body class="layout-fixed">
    <div class="wrapper">
        <!-- top navbar-->
        <header class="topnavbar-wrapper">
            <!-- Inicio Top Navbar-->
            <nav role="navigation" class="navbar topnavbar">
                <!-- Inicio navbar header-->
                <div class="navbar-header">
                    <a href="#/" class="navbar-brand">
                        <div class="brand-logo">
                            <img src="@Url.Content("~/Content/images/LogoGeneralSimple_BCO.png")" alt="App Logo" class="img-responsive" />
                        </div>
                        <div class="brand-logo-collapsed">
                            <img src="@Url.Content("~/Content/images/LogoGeneralSimple_BCO.png")" alt="App Logo" class="img-responsive" />
                        </div>
                    </a>
                </div>
                <!-- Fin navbar header-->
                <!-- Inicio Nav wrapper-->
                <div class="nav-wrapper">
                    <!-- Inicio Barra de Navegación Izquierdo-->
                    <ul class="nav navbar-nav">
                        <li>
                            <!-- Boton para mostrar/ocultar el "sidebar" en mobiles.-->
                            <a href="#" data-toggle-state="aside-toggled" data-no-persist="true" class="visible-xs sidebar-toggle">
                                <em class="fa fa-navicon"></em>
                            </a>
                        </li>
                        <li>
                            <a data-toggle="tooltip" data-placement="bottom" title="INICIO" href="@Url.Action("Index", "Home")" target="_self">
                                <em class="fa icon-home"></em>
                            </a>
                        </li>
                        <li></li>
                    </ul>


                    <!-- FIN Barra de Navegación Izquierdo-->
                    <!-- INICIO Barra de Navegación Derecho-->
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="#" data-toggle-fullscreen="" title="Pantalla Completa" data-toggle="tooltip" data-placement="bottom">
                                <em class="fa fa-expand"></em>
                            </a>
                        </li>

                        <li id="notis" class="dropdown dropdown-list">

                            <a href="#" data-toggle="dropdown" title="Notificaciones">
                                <span title="Notificaciones" data-toggle="tooltip" data-placement="bottom">
                                    <em class="fa fa-bell"></em>
                                </span>
                                <div id="total-notis" class="label label-danger n-total">0</div>
                            </a>
                            <ul class="dropdown-menu animated flipInX">
                                <li>
                                    <div id="lstNotificaciones" class="list-group">
                                        <div class="nuevos-notis">
                                        </div>
                                        <a id="mostrarnotis" href="#" class="list-group-item">
                                            <small>Más Notificaciones</small>
                                            <span id="numNotificacionestotal" class="label label-danger pull-right"></span>
                                        </a>

                                    </div>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a href="#" title="Menú sistemas" data-toggle="tooltip" data-placement="bottom">
                                <em class="icon-notebook"></em>
                            </a>
                        </li>
                        @*<li id="Alertas">
                                <a href="#" title="Alertas">
                                    <span title="Alertas" data-toggle="tooltip" data-placement="bottom">
                                        <em class="fa fa-warning"></em>
                                    </span>
                                </a>
                            </li>
                            <li>
                                <a href="#" data-toggle-state="offsidebar-open" data-no-persist="true">
                                    <span title="Más" data-toggle="tooltip" data-placement="bottom">
                                        <em class="fa fa-book"></em>
                                    </span>
                                </a>
                            </li>*@
                    </ul>
                    <!-- FIN Barra de Navegación Derecho-->
                </div>
                <!-- Fin Nav wrapper-->
            </nav>
            <!-- Fin Top Navbar-->
        </header>
        <!-- Menú lateral oculto-->
        <aside class="offsidebar hide">
            <!-- Inicio Menú lateral Derecho-->
            <nav>
                <div role="tabpanel">
                    <!-- Nav tabs-->
                    <ul role="tablist" class="nav nav-tabs nav-justified">
                        <li role="presentation" class="active">
                            <a href="#validador-doc" aria-controls="app-settings" role="tab" data-toggle="tab"></a>
                        </li>
                    </ul>
                    <!-- Tab panes-->
                    <div class="tab-content">
                        <div id="validador-doc" role="tabpanel" class="tab-pane fade in active">
                            <h3 class="text-center text-thin">ISSSTECAM</h3>
                            <div class="p">
                                <h4 class="text-muted text-thin">SISTEMAS</h4>
                                <div class="table-grid mb">
                                    <div class="col mb">
                                        <div class="setting-color">
                                            <label data-load-css="css/theme-a.css">
                                                <input type="radio" name="setting-theme" checked="checked">
                                                <%--<span class="icon-check"></span>--%>
                                    
                                                <em class="fa fa-archive icono-sistema"></em>
                                                <span class="split">
                                                    <span class="color bg-inventarios item-apps"></span>
                                                </span>
                                                <span class="color bg-white"></span>
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col mb">
                                        <div class="setting-color">
                                            <label data-load-css="css/theme-b.css">
                                                <input type="radio" name="setting-theme">
                                                <em class="fa fa-user icono-sistema"></em>
                                                <span class="split">
                                                    <span class="color bg-rh item-apps"></span>
                                                </span>
                                                <span class="color bg-white"></span>
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col mb">
                                        <div class="setting-color">
                                            <label data-load-css="css/theme-c.css">
                                                <input type="radio" name="setting-theme">
                                                <span class="icon-check"></span>
                                                <span class="split">
                                                    <span class="color bg-purple"></span>
                                                    <span class="color bg-purple-light"></span>
                                                </span>
                                                <span class="color bg-white"></span>
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col mb">
                                        <div class="setting-color">
                                            <label data-load-css="css/theme-d.css">
                                                <input type="radio" name="setting-theme">
                                                <span class="icon-check"></span>
                                                <span class="split">
                                                    <span class="color bg-danger"></span>
                                                    <span class="color bg-danger-light"></span>
                                                </span>
                                                <span class="color bg-white"></span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </nav>
            <!-- Fin Menú lateral Derecho-->
        </aside>
        <aside class="aside">
            <!-- INICIO Menu lateral Izquierdo-->
            <div class="aside-inner">
                <nav class="sidebar show-scrollbar" data-sidebar-anyclick-close="">

                    <!-- INICIO Menú lateral nav-->
                    <ul id="menu" class="nav">
                        <!-- INICIO Informacion de Usuario-->
                        <li class="has-user-block">
                            <div id="user-block">
                                <div class="item user-block">
                                    <div class="user-block-picture">
                                        <div class="user-block-status">
                                            <img src="@Url.Content("~/Content/images/escudoCampeche.png")" alt="Avatar" width="60" height="60" class="img-thumbnail img-circle">
                                            <div class="circle circle-success circle-lg"></div>
                                        </div>
                                    </div>
                                    <div class="user-block-info">
                                        <span class="user-block-name">
