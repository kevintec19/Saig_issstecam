                                  <asp:Label ID="lblPuesto" runat="server" CssClass="user-block-role"  ClientIDMode="Static"></asp:Label>*@
                                        </span>

                                    </div>
                                </div>
                            </div>
                        </li>
                        <!-- FIN Informacion Usuario-->
                        <li class="nav-heading" id="textomenu">
                            <span data-localize="sidebar.heading.HEADER">Menú</span>
                        </li>
                        <li>

                            <a href="#Catalogos" title="Catalogos" data-toggle="collapse">
                                <em class="fa fa-database"></em>
                                <span data-localize="sidebar.nav.Catalogos">Catálogos</span>
                            </a>
